package selfProgramming;

public class AssignmentConversion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int height = 157;
		double height2 = (double) height / 100;
		
		double weight = 59.7;
		
		double bmi = weight / (height2 * height2);
		
		System.out.print("Height in metres is: ");
		System.out.printf("%.2f", height2);
		System.out.print("\nBMI is: ");
		System.out.printf("%.2f", bmi);
		
		
	}

}
